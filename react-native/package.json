    "apollo-link-state": "^0.4.2",
    "apollo-link-ws": "^1.0.14",
    "apollo-storybook-react-native": "^0.2.9",
    "apollo-utilities": "^1.2.1",
    "assert": "^1.4.1",
    "deepmerge": "2.2.1",
    "eslint-plugin-react": "^7.12.4",
    "formik": "^1.4.1",
    "fraql": "^1.2.1",
    "geolib": "latest",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "messageformat": "^2.0.5",
    "moment": "^2.24.0",
    "native-base": "^2.12.0",
    "ngeohash": "^0.6.3",
    "react": "16.6.3",
    "react-apollo": "^2.5.1",
    "react-native": "0.58.6",
    "react-native-auto-height-image": "^1.0.5",
    "react-native-fbsdk": "^0.8.0",
    "react-native-firebase": "^5.2.3",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-google-places-autocomplete": "^1.3.9",
    "react-native-google-signin": "^1.0.2",
    "react-native-localize": "^1.0.1",
    "react-native-responsive-screen": "^1.2.0",
    "react-navigation": "^3.0.8",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "latest",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.2",
    "redux-saga-firebase": "^0.14.1",
    "redux-saga-rn-alert": "^1.1.5",
    "stream": "latest",
    "styled-components": "^4.1.3",
    "subscriptions-transport-ws": "^0.9.15",
    "typesafe-actions": "^3.0.0",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/es6-promise": "^3.3.0",
    "@types/jest": "^23.3.13",
    "@types/ngeohash": "^0.6.2",
    "@types/react": "^16.7.18",
    "@types/react-native": "^0.57.27",
    "@types/react-native-fbsdk": "^0.6.2",
    "@types/react-navigation": "^3.0.1",
    "@types/react-redux": "^6.0.12",
    "@types/react-test-renderer": "^16.0.3",
    "@types/redux-actions": "^2.3.1",
    "@types/redux-mock-store": "^1.0.0",
    "@types/redux-test-utils": "^0.2.0",
    "@types/storybook__react": "^4.0.0",
    "@types/storybook__react-native": "^3.0.3",
    "@types/styled-components": "^4.1.8",
    "@types/yup": "^0.26.4",
    "apollo-cache-persist": "^0.1.1",
    "apollo-storybook-decorator": "^0.5.1",
    "babel-jest": "23.6.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-redux": "latest",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.1",
    "eslint-config-react-native": "^4.0.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-react-native": "^3.5.0",
    "immutability-helper": "^3.0.0",
    "jest": "23.6.0",
    "jest-fetch-mock": "^2.1.0",
    "jest-junit": "^6.3.0",
    "metro-react-native-babel-preset": "0.51.0",
    "prettier": "^1.15.3",
