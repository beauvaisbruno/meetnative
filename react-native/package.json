{
  "name": "meetnative",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "android reload": "adb shell input text \"RR\"",
    "android": "adb reverse tcp:8097 tcp:8097 && adb reverse tcp:9090 tcp:9090 && react-native run-android",
    "android open debug menu": "adb shell input keyevent 82",
    "android build": "cd android && gradlew assembleRelease",
    "android start release": "adb reverse tcp:8097 tcp:8097 && adb reverse tcp:9090 tcp:9090 && react-native run-android --variant=release",
    "android clean": "cd android && gradlew clean",
    "stories update loader": "rnstl --pattern ./src/**/*.stories.{ts,tsx} --outputFile ./src/config/storyLoader.js",
    "test coverage": "jest --coverage --verbose --collectCoverageFrom='[\"src/**/*.{ts,tsx}\",\"!src/**/stories/*\"]'",
    "test": "jest",
    "test CI": "jest --ci --runInBand"
  },
  "dependencies": {
    "@storybook/addon-actions": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "@storybook/react-native": "^4.1.11",
