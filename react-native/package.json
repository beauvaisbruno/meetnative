    "react-native-storybook-loader": "^1.8.0",
    "react-native-typescript-transformer": "^1.2.11",
    "react-native-vector-icons": "^6.3.0",
    "react-test-renderer": "16.6.3",
    "react-testing-library": "^5.4.4",
    "reactotron-react-native": "^2.1.3",
    "reactotron-redux": "^2.1.3",
    "reactotron-redux-saga": "^2.1.3",
    "redux-mock-store": "latest",
    "redux-saga-test-plan": "latest",
    "redux-test-utils": "latest",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-blvd": "^1.2.1",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2",
    "waait": "^1.0.4"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!native-base)/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "jest/tsconfig.jest.json"
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "setupTestFrameworkScriptFile": "./jest/jest.before.js",
    "setupFiles": [
      "./jest/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*stories*"
    ]
  }
}
